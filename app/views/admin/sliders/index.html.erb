<div class="col-md-12 well">
  <%= link_to t("slider.label.new"), new_admin_slider_path %>
  <table class="table">
    <thead>      
      <th><%= Slider.human_attribute_name(:id) %></th>
      <th><%= Slider.human_attribute_name(:logo) %></th>
      <th><%= Slider.human_attribute_name(:title) %></th>
      <th><%= Slider.human_attribute_name(:show) %></th>      
      <th><%= t("link.link") %></th>      
    </thead>
    <tbody>
      <%= render @sliders %>
  </table>
  <%= will_paginate @sliders %>
</div>
